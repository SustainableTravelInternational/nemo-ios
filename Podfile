plugin 'cocoapods-binary'

platform :ios, '12.0'
use_frameworks!
enable_bitcode_for_prebuilt_frameworks!
keep_source_code_for_prebuilt_frameworks!
all_binary!

target 'NEMO' do
  pod 'SDWebImage'
  pod 'IQDropDownTextField'
  pod 'Alamofire'
  pod 'ObjectMapper'
  pod 'SCLAlertView'
  pod 'IQKeyboardManagerSwift'
  pod 'SVProgressHUD'
end
